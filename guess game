#include<stdio.h>
#include<stdlib.h>
#include<math.h>
#include<time.h>
void main()
{
	int a,b,times=0,i,j,answer[4],g[4];
	for (i=0;i<4,i++;)
	{
		srand((unsigned int)time(NULL));
		answer[i]=rand()%10;
		   for (j=0,j<i,j++;)
			   while answer[i]==answer[j];
			   {
				   answer[i]=rand()%10;
				   j=j-1;
			   }
	}
     for (i=0,i<4,i++;)
       printf("please input your guess:\n");
	    while (a<=4)
		{
		 a=0;
		 b=0;
		 times++;
		 printf("NO.%dguess:\n",times);
	     scanf("%d",&g[i]);
		 for (j=0;j<4;j++)
			 if(g[i]=answer[j])
			 {
				 b++;
				 if(i=j)
				 {
					 a++;
					 b--;
				 }
			 }
		}
}
